     Cell[BoxData[
      SubscriptBox[
       StyleBox["i", "TI"], 
       StyleBox["s", "TI"]]], "InlineFormula"],
     Cell[BoxData[
      StyleBox[")", "TI"]], "InlineFormula"],
     " such that ",
     "Underoverscript[\[Sum], k=1, s]Subscript[i, k]=Z",
     " and",
     Cell[BoxData[
      StyleBox[
       RowBox[{"T", "[", 
        RowBox[{"fs", ",", "ts", ",", "k"}], "]"}], "TI"]], "InlineFormula"],
     " is the transition probability from configuration ",
     Cell[BoxData[
      StyleBox[
       RowBox[{"(", 
        RowBox[{"0", ",", "...", ","}]}], "TI"]], "InlineFormula"],
     Cell[BoxData[
      SubscriptBox[
       StyleBox["i", "TI"], 
       StyleBox["fs", "TI"]]], "InlineFormula"],
     Cell[BoxData[
      StyleBox[
       RowBox[{
        RowBox[{"=", "k"}], ",", "...", ","}], "TI"]], "InlineFormula"],
     Cell[BoxData[
      SubscriptBox[
       StyleBox["i", "TI"], 
       StyleBox["ts", "TI"]]], "InlineFormula"],
     Cell[BoxData[
      StyleBox[
       RowBox[{
        RowBox[{
         RowBox[{"=", 
          RowBox[{"Z", "-", "k"}]}], ",", "...", ",", "0"}], ")"}], "TI"]], 
      "InlineFormula"],
     " to ",
     Cell[BoxData[
      StyleBox[
       RowBox[{"(", 
        RowBox[{"0", ",", "...", ","}]}], "TI"]], "InlineFormula"],
     Cell[BoxData[
      SubscriptBox[
       StyleBox["i", "TI"], 
